{
  "name": "www-solidaric-org",
  "version": "0.1.0",
  "description": "Website for www.solidaric.org based on Gatsby Hyperspace by HTML5 UP",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solidaric-org/www-solidaric-org.git"
  },
